<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.caleydo.view.entourage.datamapper" name="Entourage Data Mapper" schema="schema/org.caleydo.view.entourage.datamapper.exsd"/>
   <extension-point id="org.caleydo.view.entourage.pathway" name="Pathway Visualization" schema="schema/org.caleydo.view.entourage.pathway.exsd"/>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            category="org.caleydo.core.views"
            class="org.caleydo.view.entourage.RcpGLEntourageView"
            icon="resources/icons/icon.png"
            id="org.caleydo.view.entourage"
            name="Entourage">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="org.caleydo.menu.view"
               label="View">
            <menu
                  id="org.caleydo.menu.view.visualization"
                  label="Visualizations">
               <command
                     commandId="org.caleydo.view.subgraph.command.opensubgraph"
                     icon="resources/icons/icon.png"
                     style="push">
               </command>
            </menu>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.caleydo.view.entourage.command.handler.OpenSubGraphHandler"
            id="org.caleydo.view.subgraph.command.opensubgraph"
            name="Entourage">
      </command>
   </extension>
   <extension
         point="org.caleydo.view.ViewCategory">
      <ViewCategory
            isDataView="true"
            isReleaseView="true"
            viewID="org.caleydo.view.entourage">
      </ViewCategory>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.caleydo.core.gui.perspective">
         <view
               id="org.caleydo.view.entourage"
               minimized="false"
               relationship="stack"
               relative="main"
               visible="false">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.caleydo.view.ViewEmbeddingProvider">
      <Provider
            id="org.caleydo.view.entourage">
         <Embedding
               id="org.caleydo.view.entourage.pathway.multiform.level1">
         </Embedding>
         <Embedding
               id="org.caleydo.view.entourage.pathway.multiform.level2">
         </Embedding>
         <Embedding
               id="org.caleydo.view.entourage.pathway.multiform.level3">
         </Embedding>
         <Embedding
               id="org.caleydo.view.entourage.pathway.multiform.level4">
         </Embedding>
         <Embedding
               id="org.caleydo.view.entourage.path.level1">
         </Embedding>
         <Embedding
               id="org.caleydo.view.entourage.path.level2">
         </Embedding>
      </Provider>
   </extension>
   <extension
         point="org.caleydo.datadomain.pathway.embeddedPathway">
      <parent
            viewID="org.caleydo.view.entourage">
         <pathway
               embeddingID="org.caleydo.view.entourage.pathway.multiform.level4"
               icon="resources/icons/pathway_lv4.png">
            <rendererCreator
                  class="org.caleydo.view.entourage.PathwayLevel4Creator">
            </rendererCreator>
         </pathway>
      </parent>
   </extension>
   <extension
         point="org.caleydo.datadomain.pathway.PathwayAction">
      <factory
            class="org.caleydo.view.entourage.contextmenu.AddPathwayActionFactory">
      </factory>
   </extension>

</plugin>
