<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="org.caleydo.core.views"
            class="org.caleydo.view.search.RcpSearchView"
            icon="resources/icons/search.png"
            id="org.caleydo.view.search"
            name="Search">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="org.caleydo.menu.data"
               label="Search">
            <command
                  commandId="org.caleydo.view.search.command.opensearch"
                  icon="resources/icons/search.png"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.caleydo.view.search.command.handler.OpenSearchHandler"
            id="org.caleydo.view.search.command.opensearch"
            name="Search">
      </command>
   </extension>
   <extension
         point="org.caleydo.core.data.datadomain.ViewDataDomainAssociation">
      <association
            view="org.caleydo.view.search">
         <dataDomain
               type="org.caleydo.datadomain.genetic">
         </dataDomain>
      </association>
   </extension>
</plugin>
