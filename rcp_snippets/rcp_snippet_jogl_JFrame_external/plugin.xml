<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="GeneView"
            id="org.geneview.rcp.jogl.view.jframe">
      </category>
      <view
            name="SnippetJFrameView ext"
            icon="icons/sample.gif"
            category="org.geneview.rcp.jogl.view.jframe"
            class="org.geneview.rcp.jogl.view.jframe.views.SnippetJFrameView"
            id="org.geneview.rcp.jogl.view.jframe.views.SnippetJFrameView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.ui.resourcePerspective">
