<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            category="org.caleydo.core.views"
            class="org.caleydo.view.subgraph.RcpGLSubGraphView"
            icon="resources/icons/icon.png"
            id="org.caleydo.view.subgraph"
            name="Entourage">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="org.caleydo.menu.view"
               label="View">
            <menu
                  id="org.caleydo.menu.view.visualization"
                  label="Visualizations">
               <command
                     commandId="org.caleydo.view.subgraph.command.opensubgraph"
                     icon="resources/icons/icon.png"
                     style="push">
               </command>
            </menu>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.caleydo.view.subgraph.command.handler.OpenSubGraphHandler"
            id="org.caleydo.view.subgraph.command.opensubgraph"
            name="Entourage">
      </command>
   </extension>
   <extension
         point="org.caleydo.view.ViewCategory">
      <ViewCategory
            isDataView="true"
            isReleaseView="true"
            viewID="org.caleydo.view.subgraph">
      </ViewCategory>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.caleydo.core.gui.perspective">
         <view
               id="org.caleydo.view.subgraph"
               minimized="false"
               relationship="stack"
               relative="main"
               visible="false">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.caleydo.view.ViewEmbeddingProvider">
      <Provider
            id="org.caleydo.view.subgraph">
         <Embedding
               id="org.caleydo.view.subgraph.pathway.multiform.level1">
         </Embedding>
         <Embedding
               id="org.caleydo.view.subgraph.pathway.multiform.level2">
         </Embedding>
         <Embedding
               id="org.caleydo.view.subgraph.pathway.multiform.level3">
         </Embedding>
         <Embedding
               id="org.caleydo.view.subgraph.pathway.multiform.level4">
         </Embedding>
         <Embedding
               id="org.caleydo.view.subgraph.path.level1">
         </Embedding>
         <Embedding
               id="org.caleydo.view.subgraph.path.level2">
         </Embedding>
      </Provider>
   </extension>
   <extension
         point="org.caleydo.view.EmbeddedView">
      <EmbeddingInfo
            viewID="org.caleydo.view.subgraph.level4">
         <ParentView
               viewID="org.caleydo.view.subgraph">
            <Embedding
                  embeddingID="org.caleydo.view.subgraph.pathway.multiform.level4"
                  icon="resources/icons/pathway_lv4.png"
                  isDefaultView="false"
                  visInfo="org.caleydo.core.view.opengl.layout.util.multiform.DefaultVisInfo">
               <RendererCreator
                     class="org.caleydo.view.subgraph.Level4Creator">
               </RendererCreator>
            </Embedding>
         </ParentView>
      </EmbeddingInfo>
   </extension>

</plugin>
