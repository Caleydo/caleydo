<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.caleydo.view.pathway"
         name="PathwayViewCreator"
         point="org.caleydo.view.ViewCreator">
         <ViewCreator
          id="org.caleydo.view.ViewCreator"
          name="PathwayViewCreator"
          class="org.caleydo.view.pathway.creator.ViewCreator" />
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            category="org.caleydo.rcp.views"
            class="org.caleydo.view.pathway.RcpGLPathwayView"
            icon="resources/icons/pathway.png"
            id="org.caleydo.view.pathway"
            name="Single Pathway Viewer">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.caleydo.view.pathway.command.handler.OpenPathwayHandler"
            id="org.caleydo.view.pathway.command.openpathway"
            name="Single Pathway View">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="org.caleydo.menu.view"
               label="View">
            <command
                  commandId="org.caleydo.view.pathway.command.openpathway"
                  icon="resources/icons/pathway.png"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
</plugin>
